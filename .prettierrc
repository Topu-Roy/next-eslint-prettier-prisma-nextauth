{
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(@tanstack/react-router/(.*)$)|^(@tanstack/react-router$)",
    "^(@tanstack/(.*)$)|^(@tanstack$)",
    "<THIRD_PARTY_MODULES>",
    "^types$",
    "^@/lib/(.*)$",
    "^@/hooks/(.*)$",
    "^@/components/(.*)$",
    "^@/components/ui/(.*)$",
    "^[./]"
  ],
  "printWidth": 145,
  "arrowParens": "avoid",
  "trailingComma": "es5",
  "proseWrap": "never",
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
  "plugins": ["@ianvs/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"]
}
